<config>
    <modules>
        <KS_Ksall>
            <version>0.1.0</version>
        </KS_Ksall>
    </modules>
    <global>
        <helpers>
            <ksall>
                <class>KS_Ksall_Helper</class>
            </ksall>
        </helpers>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <ksall before="Mage_Adminhtml">KS_Ksall_Adminhtml</ksall>
                    </modules>
                </args>
          </adminhtml>
        </routers>
    </admin>
</config>
